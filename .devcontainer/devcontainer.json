{
	"dockerComposeFile": ["./compose.yaml"],
	"workspaceFolder": "/app",
	"service": "web",
	"runServices": ["gateway", "db", "dbgate"],
	"customizations": {
		"vscode": {
			"extensions": [
				"matthewpi.caddyfile-support",
				"svelte.svelte-vscode",
				"esbenp.prettier-vscode",
				"vitest.explorer"
			],
			"settings": {
				"remote.autoForwardPorts": false
			}
		}
	},
	"forwardPorts": ["gateway:3000"]
}
